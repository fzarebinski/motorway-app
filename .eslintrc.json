{
	"extends": [
		"airbnb-base"
	],
	"rules": {
		"indent": ["error", "tab", {"SwitchCase": 1}],
		"object-curly-spacing": ["error", "never"],
		"no-tabs": ["error", {"allowIndentationTabs": true}],
		"comma-dangle": ["error", "never"],
		"no-trailing-spaces": ["error", {"skipBlankLines": true}]
	}
}
